{
  "name": "cisco_data_service",
  "version": "0.1.0",
  "description": "It's a npm package and API wrapper implement in Typescript for Cisco Data APIs.",
  "main": "publish/src/cds.js",
  "types": "publish/src/cds.d.ts",
  "files": [
    "publish/**/*.d.ts",
    "publish/**/*.js",
    "publish/**/*.js.map"
  ],
  "license": "MIT",
  "keywords": [
    "typescript",
    "cisco",
    "api-wrapper",
    "cisco-support-api",
    "npm-package"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwangsing3/Cisco_Data_Service.git.git"
  },
  "bugs": {
    "url": "https://github.com/kwangsing3/Cisco_Data_Service/issues"
  },
  "homepage": "https://github.com/kwangsing3/Cisco_Data_Service#readme",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepublish": "tsc -p prebuild.json & del \"publish\\src\\index.d.ts\" & del /s /q \"publish\\src\\index.js\" & del /s /q \"publish\\src\\index.js.map\"",
    "build": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^14.18.5",
    "gts": "^3.1.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "qs": "^6.11.0"
  }
}
